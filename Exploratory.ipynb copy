    "Top10_Names = [x.replace('_director','') for x in Top10.index]\n",
    "\n",
    "\n",
    "fig , ax = plt.subplots(nrows=1,ncols=1,figsize= (15,10))\n",
    "ax.set_title('Top 10 director in Create Moveis',fontsize=20)\n",
